CREATE RELATION left_diagnosis([0 {1}, 1 {1}, 2 {1}, 3 {1}, 4 {1}, 5 {1}, 6 {1}, 7 {1}, 8 {1}, 9 {1}, 10 {1}, 11 {1}, 12 {1}]) {1} WITH COLUMNS (INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER)
AGG [, count] FROM (left_diagnosis([0 {1}, 1 {1}, 2 {1}, 3 {1}, 4 {1}, 5 {1}, 6 {1}, 7 {1}, 8 {1}, 9 {1}, 10 {1}, 11 {1}, 12 {1}]) {1}) GROUP BY [12] AS counts_0([12 {1}, total {1}]) {1}
CLOSEMPC counts_0([12 {1}, total {1}]) {1} INTO counts_0_close([12 {1}, total {1}]) {1, 2, 3}
CREATE RELATION right_diagnosis([0 {2}, 1 {2}, 2 {2}, 3 {2}, 4 {2}, 5 {2}, 6 {2}, 7 {2}, 8 {2}, 9 {2}, 10 {2}, 11 {2}, 12 {2}]) {2} WITH COLUMNS (INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER, INTEGER)
AGG [, count] FROM (right_diagnosis([0 {2}, 1 {2}, 2 {2}, 3 {2}, 4 {2}, 5 {2}, 6 {2}, 7 {2}, 8 {2}, 9 {2}, 10 {2}, 11 {2}, 12 {2}]) {2}) GROUP BY [12] AS counts_1([12 {2}, total {2}]) {2}
CLOSEMPC counts_1([12 {2}, total {2}]) {2} INTO counts_1_close([12 {2}, total {2}]) {1, 2, 3}
CONCATMPC [counts_0_close([12 {1}, total {1}]) {1, 2, 3}, counts_1_close([12 {2}, total {2}]) {1, 2, 3}] AS cohort([12 {}, total {}]) {1, 2, 3}
AGGMPC [total, sum] FROM (cohort([12 {}, total {}]) {1, 2, 3}) GROUP BY [12] AS counts_obl([12 {}, total {}]) {1, 2, 3}
SORTBYMPC total FROM (counts_obl([12 {}, total {}]) {1, 2, 3}) AS actual([12 {}, total {}]) {1, 2, 3}
OPENMPC actual([12 {}, total {}]) {1, 2, 3} INTO actual_open([12 {}, total {}]) {1}
