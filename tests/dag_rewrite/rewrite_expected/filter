CREATE RELATION in_1([a {1}, b {1}]) {1} WITH COLUMNS (INTEGER, INTEGER)
CLOSEMPC in_1([a {1}, b {1}]) {1} INTO in_1_close([a {1}, b {1}]) {1, 2, 3}
CREATE RELATION in_2([a {2}, b {2}]) {2} WITH COLUMNS (INTEGER, INTEGER)
CLOSEMPC in_2([a {2}, b {2}]) {2} INTO in_2_close([a {2}, b {2}]) {1, 2, 3}
CONCATMPC [in_1_close([a {1}, b {1}]) {1, 2, 3}, in_2_close([a {2}, b {2}]) {1, 2, 3}] AS rel([a {1,2}, b {1,2}]) {1, 2, 3}
FILTERMPC [a {1,2} == 42] FROM (rel([a {1,2}, b {1,2}]) {1, 2, 3}) AS filtered([a {1,2}, b {1,2}]) {1, 2, 3}
OPENMPC filtered([a {1,2}, b {1,2}]) {1, 2, 3} INTO filtered_open([a {1,2}, b {1,2}]) {1}
