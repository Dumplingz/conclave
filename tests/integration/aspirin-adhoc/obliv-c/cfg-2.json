          "password": "{{{PASSWORD}}}"
        },
        "project":
        {
          "osProjectDomain": "{{{PROJ_DOMAIN}}}",
          "osProjectName": "{{{PROJ_NAME}}}"
        },
        "data":
        {
          "container_name": "{{{DEST_CONTAINER_NAME}}}"
