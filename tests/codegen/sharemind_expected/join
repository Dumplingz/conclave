void main() {
    string ds = "DS1";
    tdbOpenConnection(ds);
    pd_shared3p uint32 [[2]] in1 = readFromDb("DS1", "in1", true);
    pd_shared3p uint32 [[2]] cl1 = in1;
    pd_shared3p uint32 [[2]] in2 = readFromDb("DS1", "in2", true);
    pd_shared3p uint32 [[2]] cl2 = in2;
    pd_shared3p uint32 [[2]] res = join(cl1, (uint){0}, cl2, (uint){0}, (uint){0,1,3});
    print("Will publish res as opened with size: ");
    print(shape(res)[0]);
    publish("opened", declassifyWrapper(res, true));

    tdbCloseConnection(ds);
}
